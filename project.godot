[application]

config/name="DatingSim"
config/description="Al's Starlight Diner is a sci-fi romance and narrative driven game set in the near future. "
run/main_scene="res://scenes/GameManager.tscn"
config/icon="res://icon.png"

[autoload]

SignalManager="*res://scripts/SignalManager.gd"
Utility="*res://scripts/Utility.gd"

[debug]

settings/physics_interpolation/enable_warnings=false

[display]

window/size/width=1080
window/size/height=720
window/size/borderless=true
window/size/fullscreen=true
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

